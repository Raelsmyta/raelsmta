<p id="notice"><%= notice %></p>
<h1>Listing Students</h1>

<table>
  <thead>
        <div class="media">
  <div class="meda-body">
    <table class ="table table-hover">
    <tr>
      <th>Student ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% sql = "Select accountNum From users" %>

<% records_array = ActiveRecord::Base.connection.execute(sql) %>
  <tbody>
     <%= link_to 'New student', new_student_path %>
    <% records_array.each do |student| %>
      <tr>
        <td><%= student[0] %></td>

        <form><td><%= link_to 'Grades', grades_path%></td></form>
      </tr>
    <% end %>
  </tbody>
</table>

<br>





